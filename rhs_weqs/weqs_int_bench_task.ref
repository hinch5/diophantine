			((/* EMPTY */)(/* EMPTY */))
			<Sim2 (t.Tag (e.LHS)(e.RHS))>
		>;
	/* 3. Расщеплять было нечего – уравнение пустое. Возвращаем все остальные уравнения. */
	(e.Result)s.Log(e.MS1)(e.MS2)((/* EMPTY */)(/* EMPTY */))(t.Tag (/* EMPTY */)(/* EMPTY */))
